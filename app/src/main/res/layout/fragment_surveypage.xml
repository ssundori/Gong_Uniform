                </LinearLayout>

                <!-- 네 번째 줄 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/q5_rb7"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="리더십" />
                    <RadioButton
                        android:id="@+id/q5_rb8"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="결정적인 순간에 활약하는 능력"
                        android:textSize="10dp"/>
                </LinearLayout>

                <!-- 다섯 번째 줄 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/q5_rb9"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="다양한 포지션을 소화하는 능력"
                        android:textSize="10dp"/>
                    <RadioButton
                        android:id="@+id/q5_rb10"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="꾸준히 우수한 실력" />
                </LinearLayout>

                <!-- 여섯 번째 줄 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/q5_rb11"
                        android:layout_width="0dp"
