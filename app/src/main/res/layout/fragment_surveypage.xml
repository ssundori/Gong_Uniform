                        android:id="@+id/q1_rb1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="매우 좋아한다" />
                    <RadioButton
                        android:id="@+id/q1_rb2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="좋아한다" />
                    <RadioButton
                        android:id="@+id/q1_rb3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="보통이다" />
